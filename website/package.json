{
  "name": "szdt-website",
  "version": "0.0.1",
  "description": "Website for SZDT",
  "homepage": "https://github.com/gordonbrander/szdt#readme",
  "bugs": {
    "url": "https://github.com/gordonbrander/szdt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gordonbrander/szdt.git"
  },
  "license": "MIT",
  "author": "Gordon Brander",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "vendor": "node scripts/cp-szdt-wasm.ts",
    "watch:ts": "esbuild --watch --bundle --minify --format=esm --target=esnext '--external:./static/vendor/*' --outdir=../docs/static src/demo.ts",
    "build:ts": "esbuild --bundle --minify --format=esm --target=esnext '--external:./vendor/*' --outfile=../docs/static/demo.js src/demo.ts",
    "build:prod": "NODE_ENV=production npx @11ty/eleventy --config=eleventy.config.ts",
    "build:dev": "npx @11ty/eleventy --config=eleventy.config.ts",
    "serve": "npx @11ty/eleventy --serve --config=eleventy.config.ts",
    "clean": "rm -rf ../docs"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "esbuild": "^0.25.8"
  }
}
